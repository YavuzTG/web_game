<!-- Whack-a-Mole SVG Icon -->
<svg width="200" height="200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="grassGrad" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#2ecc71;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#27ae60;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="holeGrad" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#2c3e50;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#34495e;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="hammerGrad" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#8b4513;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#a0522d;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background grass -->
  <rect width="200" height="200" fill="url(#grassGrad)"/>
  
  <!-- Grass texture -->
  <rect x="0" y="0" width="200" height="200" fill="none" stroke="none"/>
  
  <!-- Holes -->
  <ellipse cx="60" cy="80" rx="25" ry="20" fill="url(#holeGrad)" stroke="#8b4513" stroke-width="3"/>
  <ellipse cx="140" cy="80" rx="25" ry="20" fill="url(#holeGrad)" stroke="#8b4513" stroke-width="3"/>
  <ellipse cx="100" cy="140" rx="25" ry="20" fill="url(#holeGrad)" stroke="#8b4513" stroke-width="3"/>
  
  <!-- Mole peeking out -->
  <ellipse cx="60" cy="75" rx="18" ry="15" fill="#8B4513"/>
  <circle cx="55" cy="70" r="2" fill="black"/>
  <circle cx="65" cy="70" r="2" fill="black"/>
  <ellipse cx="60" cy="75" rx="3" ry="2" fill="pink"/>
  
  <!-- Hammer -->
  <rect x="120" y="30" width="8" height="40" fill="url(#hammerGrad)" rx="4"/>
  <rect x="115" y="25" width="18" height="15" fill="#696969" rx="3"/>
  
  <!-- Motion lines -->
  <path d="M 130 35 Q 140 25 150 30" stroke="#ffd700" stroke-width="3" fill="none" opacity="0.7"/>
  <path d="M 135 40 Q 145 30 155 35" stroke="#ffd700" stroke-width="2" fill="none" opacity="0.5"/>
  
  <!-- Score display -->
  <rect x="10" y="10" width="60" height="25" fill="rgba(255,255,255,0.9)" rx="5"/>
  <text x="40" y="27" text-anchor="middle" fill="#2c3e50" font-family="Arial, sans-serif" font-size="12" font-weight="bold">SCORE</text>
  
  <!-- Title -->
  <rect x="10" y="170" width="180" height="25" fill="rgba(255,255,255,0.9)" rx="5"/>
  <text x="100" y="187" text-anchor="middle" fill="#2c3e50" font-family="Arial, sans-serif" font-size="14" font-weight="bold">WHACK-A-MOLE</text>
</svg>
